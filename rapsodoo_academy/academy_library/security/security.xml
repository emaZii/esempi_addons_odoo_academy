<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="0">
        <record model="ir.module.category" id="module_category_library">
            <field name="name">Library</field>
            <field name="sequence">10</field>
        </record>

        <record id="group_library_user" model="res.groups">
            <field name="name">User</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="category_id" ref="module_category_library"/>
        </record>

        <record id="group_library_admin" model="res.groups">
            <field name="name">Admin</field>
            <field name="implied_ids" eval="[(4, ref('group_library_user'))]"/>
            <field name="category_id" ref="module_category_library"/>
        </record>

        <record model="res.users" id="base.user_admin">
            <field eval="[(4,ref('academy_library.group_library_admin'))]" name="groups_id"/>
        </record>
    </data>



</odoo>
